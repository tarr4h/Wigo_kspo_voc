<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grid>
    <meta>
        <deletable value="N"/>
        <rollbackable value="N"/>
        <appendable value="N"/>
        <readOnly value="N"/>
        <showCheckbox value="Y"/>
        <showNumber value="Y"/>
        <showStateBar value="Y"/>
        <showHeaderSum value="N"/>
        <showFooter value="N"/>
        <insertable value="N"/>
        <editable value="Y"/>
        <rowSelection value="N"/>
        <autoColumnWidth value="Y"/>
        <columnMove value="N"/>
        <setSort value="none"/>
    </meta>
    <column name="사용자코드" field="userCd" type="char" maxLength="20" width="80" textAlignment="left" required="Y" Pk="Y" readOnly="Y" hidden="Y"/>
    <column name="로그인ID" field="loginId" type="char" maxLength="100" width="80" textAlignment="center" required="N" Pk="Y" hidden="N"/>
    <column name="사용자명" field="userNm" type="char" maxLength="100" width="80" textAlignment="center" required="N" hidden="N"/>
    <column name="이메일" field="emailAddr" type="char" maxLength="1000" width="80" textAlignment="left" required="N" hidden="N"/>
    <column name="핸드폰" field="mphonNo" type="char" maxLength="11" width="80" textAlignment="center" required="N" hidden="Y"/>
    <column name="성별" field="userGndrCd" type="combo" maxLength="3" width="80" textAlignment="center" required="N" codeId="S040" hidden="N"/>
    <column name="생년월일" field="userBirthday" type="date" maxLength="8" width="80" textAlignment="center" required="N" hidden="N"/>
    <column name="비밀번호변경일시" field="pwdAmdDt" type="date" width="80" textAlignment="center" required="N" hidden="Y"/>
    <column name="최종로그인일시" field="lastLoginDt" type="date" width="80" textAlignment="center" required="N" readOnly="Y"  hidden="N"/>
    <column name="비밀번호만료일시" field="pwdExpDt" type="date" width="80" textAlignment="center" required="N" hidden="Y"/>
    <column name="로그인실패" field="loginFailCnt" type="int" maxLength="3" width="80" textAlignment="center" readOnly="Y"  required="N" digit="3" place="0" hidden="N"/>
    <column name="회원상태" field="delYn" type="combo" maxLength="1" width="80" textAlignment="center" required="N" codeId="S030" hidden="N"/>
    <column name="등록일시" field="regDt" type="date" width="80" textAlignment="center" required="Y"  hidden="N"/>
    <column name="수정자ID" field="amdrId" type="char" maxLength="20" width="80" textAlignment="left" required="Y" hidden="Y"/>
    <column name="수정일시" field="amdDt" type="date" width="80" textAlignment="center" required="Y" defaultValue="SYSDATE" hidden="Y"/>
    <column name="등록자ID" field="regrId" type="char" maxLength="20" width="80" textAlignment="left" required="N" hidden="Y"/>
    <column name="비밀번호초기화" displayName="초기화" field="btnRestPwd" type="button" width="60" required="N" hidden="N" />
    <column name="그룹" field="btnGroup" type="button" width="40" required="N" hidden="N" />
</grid>
    
