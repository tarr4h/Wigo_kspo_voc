<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grid>
    <meta>
        <deletable value="N"/>
        <rollbackable value="N"/>
        <appendable value="N"/>
        <readOnly value="Y"/>
        <showCheckbox value="Y"/>
        <showNumber value="Y"/>
        <showStateBar value="Y"/>
        <showHeaderSum value="N"/>
        <showFooter value="N"/>
        <insertable value="N"/>
        <editable value="Y"/>
        <rowSelection value="N"/>
        <autoColumnWidth value="Y"/>
        <columnMove value="N"/>
        <setSort value="none"/>
    </meta>
    <column name="파일코드" field="fileCd" type="char" maxLength="20" width="80" textAlignment="left" required="Y" Pk="Y" readOnly="Y" hidden="N"/>
    <column name="파일순번" field="fileOdrg" type="int" maxLength="3" width="80" textAlignment="right" required="Y" Pk="Y" readOnly="Y" digit="3" place="0" hidden="N"/>
    <column name="파일명" field="fileNm" type="char" maxLength="1000" width="80" textAlignment="left" required="N" hidden="N"/>
    <column name="파일크기" field="fileSize" type="int" maxLength="10" width="80" textAlignment="right" required="N" digit="10" place="0" hidden="N"/>
    <column name="파일저장명" field="fileSaveNm" type="char" maxLength="1000" width="80" textAlignment="left" required="N" hidden="N"/>
    <column name="파일URL" field="fileUrl" type="char" maxLength="1000" width="80" textAlignment="left" required="N" hidden="N"/>
    <column name="파일확장자명" field="fileExtNm" type="char" maxLength="100" width="80" textAlignment="left" required="N" hidden="N"/>
    <column name="MIME유형명" field="mimeTypeNm" type="char" maxLength="100" width="80" textAlignment="left" required="N" hidden="N"/>
    <column name="업로드아이디" field="amdrId" type="char" maxLength="20" width="80" textAlignment="left" required="Y" hidden="N"/>
    <column name="업로드일시" field="amdDt" type="date" width="80" textAlignment="center" required="Y" defaultValue="SYSDATE" hidden="N"/>
    <column name="관리" field="btnFileinfo" type="button" width="50" textAlignment="center"  hidden="N"/>
</grid>
